  .constant
  r 0x56
  fake_pi 3
  .endconstant

  .main
  .var
  a
  b
  .endvar
  BIPUSH 0x56
  ISTORE a
  LDC_W r
  ILOAD a
  IF_ICMPEQ l1
  GOTO l2
l1: BIPUSH 0x0 # Fake OBJREF
  ILOAD a
  LDC_W fake_pi
  INVOKEVIRTUAL .sumstuff
  ISTORE b
l2: HALT
  .endmethod

  .sumstuff(a, b)
  ILOAD a
  ILOAD b
  BIPUSH 0xF
  IADD
  IADD
  IRETURN
  .endmethod
